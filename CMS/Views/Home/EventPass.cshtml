@{
	Layout = "_UserLayout";
}
@model IEnumerable<TblEventBooking>
<!-- CSS Ticket inspired by -->
<!-- https://dribbble.com/shots/2677752-Dribbble-invite-competition -->

<div id="breadcrumb">
    <div class="container">
        <ul>
            <li><a href="#">Home</a></li>
            <li><a href="#">Entry Pass</a></li>
            <li>Page active</li>
        </ul>
    </div>
</div>

<div class="ticket">
    <div class="stub">
        <div class="top">
            <span class="admit">Admit</span>
            <span class="line"></span>
            <span class="num">
                Invitation
                <span>31415926</span>
            </span>
        </div>
        <div class="number">1</div>
        <div class="invite">
            Invite for you
        </div>
    </div>
    <div class="check">
        <div class="big">
            You're <br> Invited
        </div>
        <div class="number">#1</div>
        <div class="info">
            <section>
                <div class="title">Date</div>
                <div>4/27/2016</div>
            </section>
            <section>
                <div class="title">Issued By</div>
                <div>Ampersand</div>
            </section>
            <section>
                <div class="title">Invite Number</div>
                <div>31415926</div>
            </section>
        </div>
    </div>
</div>